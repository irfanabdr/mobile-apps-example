{
  "expo": {
    "name": "RandomUser",
    "slug": "RandomUser",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0091ea"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },

    "androidStatusBarColor": "#0064b7",
    "android": {
      "package": "com.irfan.randomuser",
      "googleServicesFile": "./google-services.json"
    }
  }
}